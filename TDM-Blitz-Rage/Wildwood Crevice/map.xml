<map proto="1.3.0">
<name>Wildwood Crevice</name>
<version>1.3.4</version>
<objective>
Be the team with the least amount of deaths after 10 minutes.
</objective>
<authors>
<author uuid="1c2664be-01ce-4be4-ac1c-a4dcc64b80c7"/>
<!--  xxjkr7xx  -->
</authors>
<contributors>
<contributor uuid="7afeaf14-7c25-48c0-ba0a-a417b25befaf" contribution="Minor aesthetics and map design"/>
<!--  goman989  -->
<contributor uuid="9d911306-eeb4-439d-badc-fa5386aa12fe" contribution="First person to notice the Kirby face"/>
<!--  XYOENDS  -->
</contributors>
<teams>
<team color="blue" max="40">Blue Team</team>
<team color="dark red" max="40">Red Team</team>
</teams>
<filters>
<filter name="only-blue">
<team>blue</team>
</filter>
<filter name="only-red">
<team>red</team>
</filter>
</filters>
<regions>
<apply block="deny-all">
<rectangle min="-oo,-oo" max="oo,oo"/>
</apply>
</regions>
<kits>
<kit name="spawn">
<item slot="0">stone sword</item>
<item slot="1" enchantment="arrow infinite:1">bow</item>
<item slot="28" amount="1">arrow</item>
<item slot="2" amount="32">cooked fish</item>
<item slot="4" amount="2">golden apple</item>
<potion duration="oo" ambient="true">speed</potion>
<potion duration="5">heal</potion>
<potion duration="10">damage resistance</potion>
</kit>
<kit name="red" parents="spawn">
<chestplate color="cd0000" damage="-3000">leather chestplate</chestplate>
<helmet color="cd0000" damage="-3000">leather helmet</helmet>
<leggings damage="-3000">iron leggings</leggings>
<boots color="cd0000" damage="-3000" enchantment="protection fall:4">leather boots</boots>
</kit>
<kit name="blue" parents="spawn">
<chestplate color="0066cc" damage="-3000">leather chestplate</chestplate>
<helmet color="0066cc" damage="-3000">leather helmet</helmet>
<leggings damage="-3000">iron leggings</leggings>
<boots color="0066cc" damage="-3000" enchantment="protection fall:4">leather boots</boots>
</kit>
</kits>
<spawns>
<spawn team="blue" kit="blue" yaw="270">
<cuboid name="mid-spawn-blue" min="-93,10,-2" max="-97,10,1"/>
</spawn>
<spawn team="red" kit="red" yaw="90">
<cuboid name="mid-spawn-red" min="75,10,1" max="79,10,-2"/>
</spawn>
<default yaw="90">
<cuboid min="-8,45,1" max="-10,45,3"/>
</default>
</spawns>
<score>
<time>10m</time>
</score>
<toolrepair>
<tool>stone sword</tool>
<tool>bow</tool>
</toolrepair>
<itemremove>
<item>leather helmet</item>
<item>leather chestplate</item>
<item>iron leggings</item>
<item>leather boots</item>
<item>arrow</item>
<item>cooked fish</item>
<item>glass bottle</item>
</itemremove>
</map>