<map proto="1.4.0"><name>Aeris: TE</name><version>1.0</version><objective>Destroy the enemy monument and leak their core!</objective><authors>    <author uuid="ee3c6f85-2254-4818-8b78-a103e87edd01"/><!-- lukbimart1 -->    <author uuid="7475c44e-3066-468f-a4c6-488fe9612097"/><!-- JcNumbers --></authors><teams>    <team id="red"  color="red"  max="5">Red</team>    <team id="blue" color="blue" max="5">Blue</team></teams><time>30m</time><timelock>on</timelock><broadcasts>    <tip after="1s">The core will turn to Emerald Blocks in `l10 minutes!</tip></broadcasts><kits>	<kit id="Team-Kit">        <potion duration="5" amplifier="10">resistance</potion>        <potion duration="oo">night_vision</potion>    </kit></kits><spawns>	<default yaw="-90">		<region>		    <cylinder base="-157.5,25,73.5" radius="5" height="1"/>		</region>	</default>	<spawn kit="Team-Kit" team="red" yaw="90">        <regions>        	<point>-71.5,24.5,42.5</point>        	<point>-71.5,24.5,43.5</point>        </regions>    </spawn>    <spawn kit="Team-Kit" team="blue" yaw="90">    	<regions>    		<point>-71.5,24.5,104.5</point>    		<point>-71.5,24.5,103.5</point>        </regions>    </spawn></spawns><regions>    <!--  Playable region  -->    <cuboid id="playable-region" min="-103,0,39" max="-153,50,108"/>    <!-- Spawn Re-Entry -->    <apply enter="never" message="You may not re-enter the spawn area!">        <region>            <cuboid min="-100,23,45" max="-70,oo,40"/>   <!-- Red Tunnel -->            <cuboid min="-100,23,106" max="-70,oo,101"/> <!--Blue Tunnel-->        </region>    </apply>    <!-- Below Bedrock region -->    <apply block-place="never" message="You may not interact with that block here!">    	<region>            <cuboid min="-104,21,101" max="-152,-oo,107"/> <!-- Blue Top Bedrock -->    	    <cuboid min="-104,21,46" max="-152,-oo,40"/>   <!-- Red Top Bedrock  -->        </region>    </apply>	<apply block="always" region="playable-region"/></regions><modes>    <mode after="10m" material="emerald_block" name="EMERALD CORE MODE"/></modes><cores material="obsidian" leak="2" mode-changes="true">    <core id="blue-core" team="blue">        <region>            <cuboid min="-141,14,95" max="-146,19,90"/>        </region>    </core>    <core id="red-core" team="red">        <region>            <cuboid min="-141,14,57" max="-146,19,52"/>        </region>    </core></cores><destroyables materials="ender_stone;beacon" completion="100%">    <destroyables owner="blue">        <destroyable name="Monument">            <region>                <cuboid min="-123,18,88" max="-120,21,91"/>            </region>        </destroyable>    </destroyables>    <destroyables owner="red">        <destroyable name="Monument">            <region>                <cuboid min="-120,18,56" max="-123,21,59"/>            </region>        </destroyable>    </destroyables></destroyables><maxbuildheight>32</maxbuildheight><itemremove>    <item>coal_ore</item>    <item>obsidian</item>    <item>ender_stone</item>    <item>beacon</item></itemremove></map>