<map proto="1.3.3">
<time result="objectives">1h</time>
<include src="../house-of-cores.xml"/>
<name>Nano</name>
<version>1.2</version>
<objective>
Destroy the enemy monuments and leak the enemy cores!
</objective>
<authors>
<author uuid="fcbc5806-e7fd-45f1-845a-22f1df93c9bf"/>
<!--  Minii_  -->
<author uuid="96c1c602-27e0-497c-b83f-1afe445cb898"/>
