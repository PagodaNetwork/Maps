<?xml version="1.0"?>
<map proto="1.4.0">
<name>CUCKOO TE</name>
<version>1.0.0</version>
<objective>Destroy the enemy's monuments</objective>
<authors>
    <author uuid="f926e3b1-ce9c-4269-a5e8-e339d5056480"/> <!-- Matilaina -->
    <author uuid="5eed208d-de58-4022-9ba7-6ccb5ea7e92a"/> <!-- Selfie -->
</authors>
<contributors>
    <contributor uuid="6e347123-c684-4d7e-996c-44d4924ff172" contribution="XML Fixes"/> <!-- SantaSoco -->
</contributors>
<destroyables completion="100%">
    <destroyables owner="blue" materials="coal block">
        <destroyable name="Up Monument"><region><cuboid min="-2,46,-17" max="1,39,-20"/></region></destroyable>
        <destroyable name="Down Monument"><region><cuboid min="-2,37,26" max="1,30,29"/></region></destroyable>
    </destroyables>
    <destroyables owner="red" materials="coal block">
        <destroyable name="Up Monument"><region><cuboid min="-51,46,26" max="-54,39,29"/></region></destroyable>
        <destroyable name="Down Monument"><region><cuboid min="-51,37,-17" max="-54,30,-20"/></region></destroyable>
    </destroyables>
</destroyables>
<teams>
   <team name="red" color="dark red" max="5">Red</team>
   <team name="blue" color="blue" max="5">Blue</team>
</teams>
<kits>
    <kit name="spawn" force="true">
        <item slot="0" material ="iron sword"/>
        <item slot="1" material ="bow"/>
        <item slot="2" material ="diamond pickaxe"/>
        <item slot="3" material ="diamond axe"/>
        <item slot="4" amount="64" material="log"/>
        <item slot="5" amount="64" material="glass"/>
        <item slot="6" amount="64" material="cobblestone"/>
        <item slot="8" amount="32" material="bread"/>
        <item slot="19" amount="64" material="arrow"/>
        <item slot="28" amount="64" material="arrow"/>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn">
        <regions yaw="-90">
            <block>-84.5,76,4.5</block> 
        </regions>
    </spawn>
    <spawn team="blue" kit="spawn">
        <regions yaw="90">
            <block>31.5,76,4.5</block> 
        </regions>
    </spawn>
    <default>
        <regions yaw="0">
            <block>-26.5,76,-62.5</block> 
        </regions>
    </default>
</spawns>
<filters>
<!-- Dispenser Filter -->
    <filter name="deny-dispensers">
        <deny>
            <block>dispenser</block>
        </deny>
    </filter>
</filters>
<regions>
<!-- Applies -->
    <apply block="never" message="You may not modify the void area!">
        <region>
            <negative>
                <rectangle min="-69,-50" max="16,59"/>
            </negative>
        </region>
    </apply>
    <apply block="never" message="You may not edit the spawn area!">
        <region>
            <region name="spawns"/>
        </region>
    </apply>
    <apply block="deny-dispensers" use="deny-dispensers">
        <region>
            <rectangle min="-oo,-oo" max="oo,oo"/>
        </region>
    </apply>
<!-- Regions -->
    <union name="spawns">
        <cuboid name="red" min="-67,41.5,7" max="-62,38,2"/>
        <cuboid name="blue" min="14,41.5,2" max="9,38,7"/>
    </union>
</regions>
<itemremove>
    <item>iron sword</item>
    <item>bow</item>
    <item>diamond pickaxe</item>
    <item>diamond axe</item>
    <item>log</item>
    <item>glass</item>
    <item>bread</item>
    <item>cobblestone</item>
    <item>coal block</item>
    <item>obsidian</item>
</itemremove>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>diamond axe</tool>
</toolrepair>
<maxbuildheight>56</maxbuildheight>
<time>30m</time>
<respawn delay="7s" blackout="true"/> <!-- Delay -->
<timelock>on</timelock>
</map>