<map proto="1.3.6">
<name>Fireside</name>
<version>1.0.0</version>
<objective>Destroy 70% of the enemy's fireplace and gifts</objective>
<authors>
	<author>iPGz</author>
    <author>MySquishyTurtle</author>
</authors>
<rules>
	<rule>Fall damage is disabled.</rule>
	<rule>You can't place blocks in the void. You can only icarus over.</rule>
</rules>
<maxbuildheight>47</maxbuildheight>
<timelock>on</timelock>
<teams>
    <team color="dark red" max="25">Red Elves</team>
    <team color="blue" max="25">Blue Elves</team>
</teams>
<destroyables name="Fireplace" completion="70%">
    <destroyable region="red-objective" materials="dark oak stairs;block coal;redstone block;iron fence;stone;cobblestone;quartz block;quartz stair;stained glass pane:4;stained glass pane:7;stained glass pane:1;wood:5;log:0;wood:1;wood:3;wool:0;wool:1;wool:4;wool:5;wool:6;wool:7;wool:8;wool:9;wool:10;wool:11;wool:14;stained clay:1;stained clay:4;stained clay:9;stained clay:14;hardened clay;leaves:0;leaves;3" owner="red"/>
    <destroyable region="blue-objective" materials="dark oak stairs;block coal;redstone block;iron fence;stone;cobblestone;quartz block;quartz stair;stained glass pane:4;stained glass pane:7;stained glass pane:1;wood:5;log:0;wood:1;wood:3;wool:0;wool:1;wool:4;wool:5;wool:6;wool:7;wool:8;wool:9;wool:10;wool:11;wool:14;stained clay:1;stained clay:4;stained clay:9;stained clay:14;hardened clay;leaves:0;leaves;3" owner="blue"/>
</destroyables>
<kits>
    <kit name="teams" parents="obs" potion-particles="false">
        <item slot="0" damage="-3000" attributes="generic.attackDamage:add:7">diamond pickaxe</item>
        <item slot="27" damage="-3000" name="`6TNT Defuser" lore="`7Right click to defuse team griefer's TNT">shears</item>
        <item slot="3" amount="64">snow block</item>
        <item slot="2">diamond spade</item>
        <item slot="18">wood sword</item>
        <item slot="30" amount="64">snow block</item>
        <item slot="21" amount="64">snow block</item>
        <item slot="12" amount="64">snow block</item>
        <item slot="17" amount="64">tnt</item>
        <item slot="1" damage="-3000" enchantment="arrow infinite:1">bow</item>
        <item slot="28">arrow</item>
        <item slot="26" amount="64">tnt</item>
        <item slot="20" amount="64">vine</item>
        <item slot="29" amount="64">vine</item>
        <item slot="35" amount="64">tnt</item>
        <item slot="4" amount="64">nether fence</item>
        <item slot="31" amount="64">step</item>
        <item slot="22" amount="16">ladder</item>
        <item slot="5" amount="16">stone button</item>
        <item slot="32" amount="8">stone plate</item>
        <item slot="23" amount="16">lever</item>
        <item slot="14" amount="16">trap door</item>
        <item slot="6">water bucket</item>
        <item slot="33">water bucket</item>
        <item slot="24">water bucket</item>
        <item slot="15" amount="16">piston base</item>
        <item slot="7" amount="16">redstone block</item>
        <item slot="34" amount="64">redstone</item>
        <item slot="25" amount="16">redstone torch on</item>
        <item slot="16" amount="16">diode</item>
        <item slot="8" amount="64">tnt</item>
        <item slot="15" amount="8">redstone comparator</item>
        <chestplate damage="-3000">iron chestplate</chestplate>
        <potion duration="oo">saturation</potion>
        <potion duration="8" amplifier="100">resistance</potion>
    </kit>
    <kit name="blue" parents="teams">
        <helmet damage="-3000" color="334CB2">leather helmet</helmet>
        <leggings damage="-3000" color="334CB2">leather leggings</leggings>
        <boots damage="-3000" color="334CB2">leather boots</boots>
    </kit>
    <kit name="red" parents="teams">
        <helmet damage="-3000" color="dd5566">leather helmet</helmet>
        <leggings damage="-3000" color="dd5566">leather leggings</leggings>
        <boots damage="-3000" color="dd5566">leather boots</boots>
    </kit>
</kits>
<killreward>
    <item amount="32">tnt</item>
</killreward>
<spawns>
    <spawn team="red" yaw="180" kit="red">
        <region><cuboid min="-13,62,48" max="14,59,45"/></region>
    </spawn>
    <spawn team="blue" yaw="0" kit="blue">
        <region><cuboid min="-13,62,-48" max="14,59,-45"/></region>
    </spawn>
    <default yaw="90">
        <point>51,16,0.5</point>
    </default>
</spawns>
<filters>
    <filter name="deny-void">
        <deny>
            <void/>
        </deny>
    </filter>
    <filter name="floor">
        <any>
            <block>quartz block</block>
            <all>
                <block>stained clay</block>
                <cause>player</cause>
            </all>
        </any>
    </filter>
    <filter name="no-water">
        <deny>
            <block>water</block>
            <block>stationary water</block>
        </deny>
    </filter>
    <filter name="deny-blue" parents="deny-blocks">
        <deny>
            <team>blue</team>
        </deny>
    </filter>
    <filter name="deny-red" parents="deny-blocks">
        <deny>
            <team>red</team>
        </deny>
    </filter>
</filters>
<regions>
    <apply filter="no-water" message="You may not place water near the objective.">
        <region><cuboid name="blue-objective" min="-32,10,-31" max="32,54,-51"/></region>
    </apply>
    <apply filter="floor">
        <region><cuboid name="blue-floor" min="33,10,-52" max="-32,0,-15"/></region>
    </apply>
        <region><cuboid name="blue-back" min="-32,10,-52" max="33,61,-51"/></region>
    <apply filter="deny-red" message="You may not enter the blue team's spawn.">
        <region><cuboid name="blue-spawn" min="-18,55,-39" max="20,62,-51"/></region>
    </apply>
    <apply filer="no-water" message="You may not place water near the objective.">
        <region><cuboid name="red-objective" min="-32,10,31" max="32,54,51"/></region>
    </apply>
    <apply filter="floor">			
        <region><cuboid name="red-floor" min="33,10,52" max="-32,0,15"/></region>
    </apply>
    <apply filter="back" message="You may not edit the back wall.">
        <region><cuboid name="red-back" min="-32,10,52" max="33,61,51"/></region>
    </apply>
    <apply filter="deny-blue" message="You may not enter the red team's spawn.">
        <region><cuboid name="red-spawn" min="-18,55,-9" max="20,62,51"/></region>
    </apply>
    <union name="map">
        <region><rectangle name="blue-side" min="-33,-15" max="-32,-52"/></region>
        <region><rectangle name="red-side" min="-33,15" max="-32,52"/></region>
    </union>
    <apply filter="deny-void" message="You may not edit the void area.">
       <union>
            <negative>
                 <region name="map"/>
            </negative>
       </union>
   </apply>
    <apply enter="deny-all" message="You may not edit the observer spawn.">
        <region><cuboid name="obs-spawn" min="44,6,-25" max="=64,43,25"/></region>
    </apply>
</regions>
<blockdrops>
    <rule>
        <filter>
            <block>snow block</block>
        </filter>
        <drops>
            <item>snow block</item>
        </drops>
    </rule>
</blockdrops>
<itemremove>
    <item>diamond pickaxe</item>
    <item>diamond spade</item>
    <item>vine</item>
    <item>stained clay</item>
    <item>stained glass</item>
    <item>nether fence</item>
    <item>step</item>
    <item>ladder</item>
    <item>trap door</item>
    <item>stone plate</item>
    <item>piston base</item>
    <item>redstone block</item>
    <item>redstone</item>
    <item>stone button</item>
    <item>lever</item>
    <item>redstone torch on</item>
    <item>diode</item>
    <item>tnt</item>
    <item>redstone comparator</item>
    <item>dispenser</item>
    <item>slime ball</item>
    <item>fence gate</item>
    <item>piston sticky base</item>
    <item>chainmail chestplate</item>
    <item>diode</item>
    <item>bow</item>
    <item>arrow</item>
    <item>shears</item>
    <item>snow block</item>
    <item>wood</item>
    <item>stained glass pane</item>
    <item>iron fence</item>
    <item>leaves</item>
    <item>quartz stairs</item>
    <item>quartz block</item>
    <item>cobblestone</item>
    <item>stone</item>
    <item>clay</item>
    <item>hard clay</item>
    <item>dark oak stairs</item>
</itemremove>
</map>