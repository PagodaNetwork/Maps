<?xml version="1.0"?>
<map proto="1.0.0">
<name>War Wars</name>
<version>150</version>
<objective> Leak lava from the enemy's obsidian core into the void. </objective>
-<authors>
	<author uuid="177803b2-797c-4089-aae1-5702ca259e2c"/>
	<!-- Onsole -->
</authors>
-<rules>
	<rule>No building at spawn</rule>
	<rule>No fighting at spawn</rule>
	<rule>No using a bucket on lava</rule>
	<rule>No damaging your own core</rule>
</rules>
-<mobs>
	-<filter>
		<spawn>spawner</spawn>
	</filter>
</mobs>
-<teams>
	<team max="5" color="blue">Blue</team>
	<team max="5" color="dark red">Red</team>
</teams>
<regions>
	<negative name="outside">
		<rectangle max="46,45" name="playable" min="-4,16"/>
	</negative>
</regions>
<portals>
	<portal x="24" y="22.5" z="37">
		<region name="outside"/>
	</portal>
</portals>
-<blitz>
	<time>10m</time>
	<lives>1</lives>
</blitz>
-<spawns>
	<spawn yaw="90" team="blue">
		<block>29.5,22,29.5</block>
	</spawn>
	<spawn yaw="-90" team="red">
		<block>17.5,22,29.5</block>
	</spawn>
	<default yaw="180">
		<block>24,22.5,37</block>
	</default>
</spawns>
<cores leak="3" material="obsidian">
	<core team="blue">
		<cuboid max="34,30,31" min="31,27,28"/>
	</core>
	<core team="red">
		<cuboid max="17,30,31" min="14,27,28"/>
	</core>
</cores>
</map>