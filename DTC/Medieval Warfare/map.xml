<?xml version="1.0"?>
<map proto="1.0.0">
<name>Medieval Warfare</name>
<version>1.6.1</version>
<objective>Leak lava from the enemy's obsidian core.</objective>
<authors>
    <author>Daffy_Duck01</author>
</authors>
<rules>
    <rule>No team griefing!</rule>
</rules>
<teams>
    <team color="blue" max="32">Blue Team</team>
    <team color="dark red" max="32">Red Team</team>
</teams>
<regions>
    <apply block="deny-all">
        <cuboid name="blue-spawn-protect" min="57,93,74" max="63,oo,80"/>
        <cuboid name="red-spawn-protect" min="-84,93,74" max="-78,oo,80"/>
    </apply>
</regions>
<spawns>
    <spawn team="blue" yaw="90"><cuboid min="66.5,157,75.5" max="69.5,157,78.5"/></spawn>
    <spawn team="red" yaw="270"><cuboid min="-90.5,157,75.5" max="-87.5,157,78.5"/></spawn>
    <default yaw="180"><cylinder base="-11.5,90,-32.5" radius="3" height="0"/></default>
</spawns>
<playable>
    <cuboid min="-155,-oo,3" max="134,156,172"/>
</playable>
<maxbuildheight>127</maxbuildheight>
<cores material="obsidian" leak="3">
    <core team="blue"><cuboid min="30,97,75" max="34,101,79"/></core>
    <core team="red"><cuboid min="-55,97,75" max="-51,101,79"/></core>
</cores>
</map>
