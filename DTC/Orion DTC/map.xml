<?xml version="1.0"?>
<map proto="1.4.0">
<name>Orion</name>
<version>1.0.0</version>
<objective>Leak the enemy team's core!</objective>
<authors>
    <author uuid="b03360db-b8cd-49de-8ba1-b7920c2238a9"/> <!--  Obelistics  -->
</authors>
<contributors>
    <contributor uuid="a23a1d91-7e97-40b4-8f5e-1ab82d8f5ade" contribution="XML"/> <!--  chle_  -->
</contributors>
<teams>
    <team id="red" color="dark red" max="5">Red</team>
    <team id="blue" color="blue" max="5">Blue</team>
</teams>
<time result="objectives">40m</time>
<respawn delay="5s"/>
<spawns>
    <spawn team="red" kit="red-kit">
        <regions yaw="180">
            <block>7.5,91.1,99.5</block>
        </regions>
    </spawn>
    <spawn team="blue" kit="blue-kit">
        <regions yaw="0">
            <block>7.5,91.1,-92.5</block>
        </regions>
    </spawn>
    <default>
        <regions yaw="0">
            <cuboid min="6.5,38.1,2.5" max="8.5,38.1,4.5"/>
        </regions>
    </default>
</spawns>
<kits>
    <kit id="everyone-kit">
        <potion duration="oo" amplifier="1">night vision</potion>
    </kit>
    <kit id="spawn-kit" parents="everyone-kit">
        <item slot="0">iron sword</item>
        <item slot="1" enchantment="arrow infinite">bow</item>
        <item slot="2">diamond pickaxe</item>
        <item slot="3">iron axe</item>
        <item slot="4" amount="16" damage="1">log 2</item>
        <item slot="6">golden apple</item>
        <item slot="7" amount="32">cooked fish</item>
        <item slot="28">arrow</item>
    </kit>
    <kit id="red-kit" parents="spawn-kit">
        <helmet damage="14">stained glass</helmet>
        <item slot="5" amount="64" damage="14">stained clay</item>
    </kit>
    <kit id="blue-kit" parents="spawn-kit">
        <helmet damage="11">stained glass</helmet>
        <item slot="5" amount="64" damage="11">stained clay</item>
    </kit>
</kits>
<killreward>
    <item>golden apple</item>
</killreward>
<maxbuildheight>30</maxbuildheight>
<timelock>on</timelock>
<cores material="obsidian" leak="8">
    <core team="red" name="Core A">
        <region>
            <cuboid min="12,4,-38" max="-21,13,-46"/>
        </region>
    </core>
    <core team="red" name="Core B">
        <region>
            <cuboid min="36,4,-38" max="28,13,-47"/>
        </region>
    </core>
    <core team="blue" name="Core A">
        <region>
            <cuboid min="28,4,45" max="36,13,53"/>
        </region>
    </core>
    <core team="blue" name="Core B">
        <region>
            <cuboid min="-21,4,45" max="-13,13,53"/>
        </region>
    </core>
</cores>
<destroyables name="void gap viewer" materials="stained glass:0" completion="0%" show="false" mode-changes="true">
    <destroyable owner="attackers">
        <region>
            <everywhere/>
        </region>
    </destroyable>
</destroyables>
<modes>
    <mode after="0s" material="piston moving piece"/>
</modes>
<filters>
    <not id="deny-void">
        <void/>
    </not>
</filters>
<regions>
    <negative id="yes-void">
        <rectangle min="-4,74" max="-25,41"/>
        <rectangle min="18,75" max="40,41"/>
        <rectangle min="40,-34" max="19,-67"/>
        <rectangle min="-4,-67" max="-25,-34"/>
    </negative>
    <union id="spawns">
        <rectangle min="2.5,76.5" max="12.5,104.5"/>
        <rectangle min="12.5,-69.5" max="2.5,-103.5"/>
    </union>
    <apply block="deny-void" region="yes-void" message="You may not build in the void!"/>
    <apply block="never" region="spawns" message="You may not destroy the spawn area!"/>
</regions>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
</toolrepair>
<itemremove>
    <item>iron sword</item>
    <item>bow</item>
    <item>diamond pickaxe</item>
    <item>iron axe</item>
    <item>stained glass</item>
    <item>stained clay</item>
    <item>log 2</item>
    <item>golden apple</item>
    <item>cooked fish</item>
    <item>obsidian</item>
</itemremove>
</map>