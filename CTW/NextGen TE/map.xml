<?xml version="1.0"?>
<map proto="1.3.5">
	<autorespawn/>
	<time result="objectives">45m</time>
	<name>NextGen TE</name>
	<version>3.1</version>
	<objective>Take the enemy's wool located to either side of the enemy's base and place it in your victory monument.</objective>
	<authors>
		<author contribution="reinventing the map" uuid="dad8b95c-cf6a-44df-982e-8c8dd70201e0"/>
		<!-- ElectroidFilms -->
	</authors>
	<contributors>
		<contributor contribution="re-reinventing the map" uuid="cbdc7b1d-7a07-465b-bebb-4f20bfb8ebd8"/>
		<!-- Cratain -->
		<contributor contribution="bias" uuid="621b2aa8-4e5f-4ed8-bd36-0ae2f8952a06"/>
		<contributor contribution="old map design" uuid="ef4ea031-998f-4ec9-b7b6-1bdd428bcef8"/>
		<!-- Plastix -->
		<contributor contribution="old map design" uuid="82c796a5-c033-43be-af30-fa06496995f9"/>
		<!-- IM_A_H0B0 -->
		<contributor contribution="old map design and gameplay management" uuid="3c7db14d-ac4b-4e35-b2c6-3b2237f382be"/>
		<!-- MonsieurApple -->
		<contributor contribution="old map design and item management" uuid="25961a08-c90c-4abd-b136-dad90e89c2eb"/>
		<!-- Anxuiz -->
	</contributors>
	<teams>
		<team color="blue" id="Blue" max="10">Blue</team>
		<team color="dark red" id="Red" max="10">Red</team>
	</teams>
	<spawns>
		<spawn team="red" yaw="180">
			<point>-158.5,4,69.5</point>
		</spawn>
		<spawn team="blue" yaw="0">
			<point>-158.5,4,-42.5</point>
		</spawn>
		<default yaw="-90">
			<cylinder base="-158.5,51,13.5" height="0" radius="3"/>
		</default>
	</spawns>
	<filters>
		<!-- Team Filters -->
		<filter name="only-blue">
			<team>blue</team>
		</filter>
		<filter name="only-red">
			<team>red</team>
		</filter>
		<!-- Block Filters -->
		<filter name="deny-void">
			<deny>
				<void/>
			</deny>
		</filter>
		<filter name="only-iron">
			<block>iron block</block>
		</filter>
		<!-- Usage Filters -->
		<filter name="not-chest">
			<not>
				<block>chest</block>
			</not>
		</filter>
		<filter name="not-glass">
			<not>
				<block>glass</block>
			</not>
		</filter>
		<filter name="only-red-usage">
			<not>
				<all>
					<block>chest</block>
					<team>blue</team>
				</all>
			</not>
		</filter>
		<filter name="only-blue-usage">
			<not>
				<all>
					<block>chest</block>
					<team>red</team>
				</all>
			</not>
		</filter>
	</filters>
	<block-drops>
		<rule wrong-tool="true">
			<region>
				<rectangle max="oo,oo" min="-oo,-oo"/>
			</region>
			<filter>
				<any>
					<material>gravel</material>
				</any>
			</filter>
			<drops>
				<item material="flint"/>
			</drops>
		</rule>
	</block-drops>
	<regions>
		<!-- Region Definitions -->
		<rectangle max="oo,oo" min="-oo,-oo" name="all"/>
		<union name="void-exemptions">
			<rectangle max="-133,-43" min="-118,70"/>
			<rectangle max="-199,-43" min="-184,70"/>
			<rectangle max="-163,-4" min="-184,31"/>
			<rectangle max="-154,31" min="-133,-4"/>
			<rectangle max="-154,20" min="-163,7"/>
			<rectangle max="-154,-21" min="-163,-28"/>
			<rectangle max="-154,55" min="-163,48"/>
		</union>
		<union name="spawns">
			<cuboid max="-173,9,80" min="-144,0,56" name="red-spawn"/>
			<mirror name="blue-spawn" normal="0,0,-1" origin="-158.5,0,13.5" region="red-spawn"/>
		</union>
		<union name="wool-rooms">
			<union name="blue-rooms">
				<union name="yellow-room">
					<rectangle max="-118,86" min="-132,96"/>
					<rectangle max="-118,86" min="-131,85"/>
					<rectangle max="-118,86" min="-130,84"/>
					<rectangle max="-118,86" min="-129,83"/>
					<rectangle max="-118,86" min="-128,82"/>
				</union>
				<union name="purple-room">
					<rectangle max="-199,86" min="-185,96"/>
					<rectangle max="-199,86" min="-186,85"/>
					<rectangle max="-199,86" min="-187,84"/>
					<rectangle max="-199,86" min="-188,83"/>
					<rectangle max="-199,86" min="-189,82"/>
				</union>
			</union>
			<union name="red-rooms">
				<union name="orange-room">
					<rectangle max="-199,-59" min="-185,-69"/>
					<rectangle max="-199,-59" min="-186,-58"/>
					<rectangle max="-199,-59" min="-187,-57"/>
					<rectangle max="-199,-59" min="-188,-56"/>
					<rectangle max="-199,-59" min="-189,-55"/>
				</union>
				<union name="lime-room">
					<rectangle max="-118,-59" min="-132,-69"/>
					<rectangle max="-118,-59" min="-131,-58"/>
					<rectangle max="-118,-59" min="-130,-57"/>
					<rectangle max="-118,-59" min="-129,-56"/>
					<rectangle max="-118,-59" min="-128,-55"/>
				</union>
			</union>
		</union>
		<union name="wool-placements">
			<block location="-159.5,5.5,78.5" name="orange-wool"/>
			<block location="-157.5,5.5,78.5" name="lime-wool"/>
			<block location="-159.5,5.5,-51.5" name="purple-wool"/>
			<block location="-157.5,5.5,-51.5" name="yellow-wool"/>
		</union>
		<!-- Block Rules -->
		<apply block-place="not-glass" message="You may not place glass in the side lanes!">
			<rectangle max="-133,-43" min="-118,70"/>
			<rectangle max="-199,-43" min="-184,70"/>
		</apply>
		<apply block="deny-all" message="You may not edit the spawn area!">
			<region name="spawns"/>
		</apply>
		<apply block-place="deny-void" message="You may not build outside the map!">
			<negative>
				<region name="void-exemptions"/>
			</negative>
		</apply>
		<apply block="only-red" message="You may not edit the enemy wool room!">
			<region name="red-rooms"/>
		</apply>
		<apply block="only-blue" message="You may not edit the enemy wool room!">
			<region name="blue-rooms"/>
		</apply>
		<!-- Entry Rules -->
		<apply enter="only-red" message="You may not enter the enemy wool room!">
			<region name="red-rooms"/>
		</apply>
		<apply enter="only-blue" message="You may not enter the enemy wool room!">
			<region name="blue-rooms"/>
		</apply>
		<!-- Usage Rules -->
		<apply message="You may not interact with the enemy wool room!" use="only-blue-usage">
			<region name="blue-rooms"/>
		</apply>
		<apply message="You may not interact with the enemy wool room!" use="only-red-usage">
			<region name="red-rooms"/>
		</apply>
	</regions>
	<wools craftable="false">
		<wool color="purple" location="-193.5,14.5,90.5" team="blue">
			<region name="purple-wool"/>
		</wool>
		<wool color="yellow" location="-123.5,14.5,90.5" team="blue">
			<region name="yellow-wool"/>
		</wool>
		<wool color="lime" location="-123.5,14.5,-63.5" team="red">
			<region name="lime-wool"/>
		</wool>
		<wool color="orange" location="-193.5,14.5,-63.5" team="red">
			<region name="orange-wool"/>
		</wool>
	</wools>
	<itemremove>
		<item>string</item>
		<item>glowstone dust</item>
		<item>apple</item>
		<item>sapling</item>
		<item>sugar cane</item>
		<item>clay ball</item>
		<item>stone plate</item>
		<item>sand</item>
		<item>iron sword</item>
		<item>bow</item>
		<item>iron pickaxe</item>
		<item>iron spade</item>
		<item>log 2</item>
		<item>wood</item>
		<item>glass</item>
		<item>glass bottle</item>
		<item>cooked fish</item>
		<item>redstone</item>
	</itemremove>
	<maxbuildheight>28</maxbuildheight>
	<gamerules>
		<doDaylightCycle>true</doDaylightCycle>
	</gamerules>
	<respawn delay="5s"/>
</map>