    </filter>
    <filter name="deny-obsidian">
        <deny>
            <block>obsidian</block>
        </deny>
    </filter>
    <filter name="deny-void">
        <deny>
            <void/>
        </deny>
    </filter>
    <filter name="deny-wool">
        <deny>
            <block>wool</block>
        </deny>
    </filter>
    <filter name="only-red">
        <team>red</team>
    </filter>
    <filter name="only-blue">
        <team>blue</team>
    </filter>    
    <filter name="only-yellow">
        <team>yellow</team>
    </filter>
    <filter name="only-green">
        <team>green</team>
    </filter>
</filters>
