<teams>
    <team color="yellow" max="5">Yellow Team</team>
    <team color="dark green" max="5">Green Team</team>
    <team color="dark red" max="5">Red Team</team>
    <team color="blue" max="5">Blue Team</team>
</teams>
<spawns>
    <spawn team="red" kit="red" yaw="180">
        <point>0.5,231,101.5</point>
    </spawn>
    <spawn team="green" kit="green" yaw="90">
